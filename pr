    %inv;
    %addi 1, 0, 3;
    %store/vec4 v0x55e5a7d03e90_0, 0, 3;
    %load/vec4 v0x55e5a7d03ba0_0;
    %pad/u 32;
    %load/vec4 v0x55e5a7d03e90_0;
    %pad/u 32;
    %add;
    %cmpi/e 0, 0, 32;
    %jmp/0xz  T_0.2, 4;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e5a7d03a40_0, 0, 1;
    %jmp T_0.3;
T_0.2 ;
    %pushi/vec4 0, 0, 1;
    %store/vec4 v0x55e5a7d03a40_0, 0, 1;
T_0.3 ;
T_0.0 ;
    %pushi/vec4 1, 0, 1;
    %store/vec4 v0x55e5a7d03a40_0, 0, 1;
    %jmp T_0;
