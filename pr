E_0x55e5a7cf1de0 .event edge, v0x55e5a7d03980_0;
    .scope S_0x55e5a7cba0b0;
T_0 ;
    %wait E_0x55e5a7cf1de0;
    %load/vec4 v0x55e5a7d03980_0;
